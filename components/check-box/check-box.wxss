.main {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.title {
  font-size: 30rpx;
  font-family: -apple-system;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

.check-field {
  width: 100%;
  height: 450rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  align-content: space-around;
  /* border: 1px solid blue; */
}

.check-list {
  width: 22%;
  height: 60rpx;
  border-radius: 40rpx;

  font-size: 30rpx;
  /*水平居中*/
  text-align: center;
  /*垂直居中*/
  line-height: 60rpx;
  font-family: -apple-system;

  background-color: #ffffff;
  color: #999999;
  border: 1px solid #DBDBDB;
}

.selected {
  background-color: #F86E6E;
  color: rgba(255, 255, 255, 1);
  border: 1px solid #F86E6E;
}